[BASIC]
# No docstring required for internal methods or unit tests
no-docstring-rgx=__.*
# x and y are perfect for coordinates
# to is valid name
# v, n ok for vertex & neighbor
good-names=i,j,k,ex,Run,_,x,y,to,it,op,v,n,s


[MESSAGES CONTROL]
# Disable since code is bound to be duplicated with many different
# implementations of the same thing
# Disable warning about locally disabled warnings
disable=duplicate-code, I0011, I0012
